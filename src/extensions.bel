(with (doc (list (uvar) "This function is implemented natively in Chime.")
       hook (car:1:3 time))
  (each nm native-ops
    (set
      (let op ((macro () nm))
        (if (isa!mac op)
            (cddr:3 op)
            (cddr op)))
      `(((,hook . ,nm) ,doc) () ()))))

(def tabref2 (tab key (o default))
   (ifwhere (cdr:tabloc tab key) (tabref tab key)))

(vir tab (f args)
  `(tabref2 ,f ,@args))
